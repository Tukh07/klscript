TabHolder.Size = UDim2.new(1, -20, 0, 25); TabHolder.Position = UDim2.new(0, 10, 0, 0); TabHolder.BackgroundTransparency = 1

local SeaTabBtn = Instance.new("TextButton", TabHolder)
SeaTabBtn.Size = UDim2.new(0.48, 0, 1, 0); SeaTabBtn.Text = "SEA EVENT"; SeaTabBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 40); SeaTabBtn.TextColor3 = Color3.new(1,1,1); SeaTabBtn.Font = Enum.Font.GothamBold; Instance.new("UICorner", SeaTabBtn)

local SettingTabBtn = Instance.new("TextButton", TabHolder)
SettingTabBtn.Size = UDim2.new(0.48, 0, 1, 0); SettingTabBtn.Position = UDim2.new(0.52, 0, 0, 0); SettingTabBtn.Text = "SETTINGS"; SettingTabBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 25); SettingTabBtn.TextColor3 = Color3.fromRGB(150, 150, 150); SettingTabBtn.Font = Enum.Font.GothamBold; Instance.new("UICorner", SettingTabBtn)

