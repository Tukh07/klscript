repeat task.wait() until game:IsLoaded()

-- ================= C·∫§U H√åNH T·ªåA ƒê·ªò =================
local TargetPos = CFrame.new(2234.82, 363.26, -1202.24) -- T·ªça ƒë·ªô b·∫°n ƒë√£ cho

-- ================= H√ÄM H·ªñ TR·ª¢ & CONFIG =================
local function GetRequest()
    local req = (syn and syn.request) or (http and http.request) or http_request or (fluxus and fluxus.request) or request
    return req
end

local RemoteCommandURL = "https://gist.githubusercontent.com/Tukh07/a98a3af61e3a4473d207f214cf333718/raw/fe994b2ccebb7b40616d1268d9e1c2d4588ffb71/jobid.txt"
local LastRemoteJobID = ""
local WebhookURL = "https://discord.com/api/webhooks/1459571921544024228/ow02aO64-Cz56PqZJWChetyPvwJzzjVkjYrDF1iZ8KhzvdGrra8Q5z2w9lq5koJtPHFh"
local LastTargetName = "" 

local function SendWebhook(title, msg, color)
    local data = {
        ["embeds"] = {{
            ["title"] = title, ["description"] = msg, ["color"] = color or 65535,
            ["footer"] = {["text"] = "Time: " .. os.date("%X")},
            ["author"] = {["name"] = "Player: " .. game.Players.LocalPlayer.Name}
        }}
    }
    task.spawn(function()
        local request_func = GetRequest()
        if request_func then
            request_func({Url = WebhookURL, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = game:GetService("HttpService"):JSONEncode(data)})
        end
    end)
end

local ConfigFile = "SeaFarm_Ultimate_Config.json"
local Config = {
    AutoSkill = false, FarmSeaEvent = false, WeaponMode = "Sword",
    AutoArm = true, AutoKen = true, AutoServerHop = false
}

local function SaveConfig() pcall(function() writefile(ConfigFile, game:GetService("HttpService"):JSONEncode(Config)) end) end
pcall(function() if isfile(ConfigFile) then local data = game:GetService("HttpService"):JSONDecode(readfile(ConfigFile)) for k,v in pairs(data) do Config[k] = v end end end)

-- ================= SERVICES =================
local Players = game:GetService("Players")
local RS = game:GetService("ReplicatedStorage")
local VIM = game:GetService("VirtualInputManager")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")
local TweenService = game:GetService("TweenService")
local GuiService = game:GetService("GuiService")
local Player = Players.LocalPlayer

-- ================= AUTO REJOIN =================
task.spawn(function()
    GuiService.ErrorMessageChanged:Connect(function()
        local msg = GuiService:GetErrorMessage()
        if msg ~= "" then
            task.wait(5)
            TeleportService:Teleport(game.PlaceId, Player)
        end
    end)
end)

-- ================= GUI SYSTEM =================
local gui = Instance.new("ScreenGui", Player.PlayerGui)
gui.Name = "SeaFarm_Titan_Edition"; gui.ResetOnSpawn = false

local MainFrame = Instance.new("Frame", gui)
MainFrame.Size = UDim2.fromScale(0.25, 0.65); MainFrame.Position = UDim2.fromScale(0.05, 0.2)
MainFrame.BackgroundColor3 = Color3.fromRGB(12, 12, 18); MainFrame.Active = true; MainFrame.Draggable = true; MainFrame.BorderSizePixel = 0
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 12)

local UIStroke = Instance.new("UIStroke", MainFrame)
UIStroke.Thickness = 2; UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
local UIGradient = Instance.new("UIGradient", UIStroke)
UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0, Color3.fromRGB(255,0,0)), ColorSequenceKeypoint.new(1, Color3.fromRGB(255,0,255))}
task.spawn(function() while task.wait() do UIGradient.Rotation = UIGradient.Rotation + 1 end end)

local Title = Instance.new("TextLabel", MainFrame)
Title.Size = UDim2.new(0.8, 0, 0.15, 0); Title.Position = UDim2.new(0, 15, 0, 0)
Title.Text = "SEA EVENT <font color='#00FFFF'>HOME MADE</font>"; Title.RichText = true
Title.TextColor3 = Color3.new(1, 1, 1); Title.BackgroundTransparency = 1
Title.TextScaled = true; Title.TextXAlignment = Enum.TextXAlignment.Left; Title.Font = Enum.Font.GothamBlack

local IsMinimized = false
local MinBtn = Instance.new("TextButton", MainFrame)
MinBtn.Size = UDim2.new(0, 30, 0, 30); MinBtn.Position = UDim2.new(1, -35, 0.5, -15)
MinBtn.Text = "-"; MinBtn.BackgroundColor3 = Color3.fromRGB(255, 60, 60); MinBtn.TextColor3 = Color3.new(1, 1, 1); MinBtn.Font = Enum.Font.GothamBold; Instance.new("UICorner", MinBtn)

local ContentGroup = Instance.new("Frame", MainFrame)
ContentGroup.Size = UDim2.new(1, 0, 0.85, 0); ContentGroup.Position = UDim2.new(0, 0, 0.15, 0); ContentGroup.BackgroundTransparency = 1

local function ToggleMinimize(state)
    IsMinimized = state
    MainFrame:TweenSize(IsMinimized and UDim2.fromScale(0.25, 0.06) or UDim2.fromScale(0.25, 0.65), "Out", "Back", 0.4, true)
    ContentGroup.Visible = not IsMinimized
    MinBtn.Text = IsMinimized and "+" or "-"
    MinBtn.BackgroundColor3 = IsMinimized and Color3.fromRGB(0, 200, 100) or Color3.fromRGB(255, 60, 60)
    MinBtn.Position = IsMinimized and UDim2.new(1, -35, 0.5, -15) or UDim2.new(1, -35, 0, 5)
    Title.Size = IsMinimized and UDim2.new(0.8, 0, 1, 0) or UDim2.new(0.8, 0, 0.15, 0)
    if not IsMinimized then 
        Title.Text = "SEA EVENT <font color='#00FFFF'>HOME MADE</font>" 
    elseif LastTargetName ~= "" then 
        Title.Text = "üéØ <font color='#FFA500'>" .. LastTargetName:upper() .. "</font>"
    else
        Title.Text = "üì° SEARCHING..."
    end
end
MinBtn.MouseButton1Click:Connect(function() ToggleMinimize(not IsMinimized) end)

-- Tab System
