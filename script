TeleBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 220); TeleBtn.Text = "JOIN B·∫∞NG ID"; TeleBtn.TextColor3 = Color3.new(1, 1, 1)
TeleBtn.Font = Enum.Font.GothamBold; Instance.new("UICorner", TeleBtn).CornerRadius = UDim.new(0, 6)
TeleBtn.MouseButton1Click:Connect(function() if JobInput.Text ~= "" then game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, JobInput.Text, Player) end end)

-- ================= LOGIC FARM (GI·ªÆ NGUY√äN 100% - CH·ªà FIX STATUS DISPLAY) =================
local CurrentWeaponIdx = 1; local WeaponTypes = {"Sword", "Combat"}; local NoMonsterTimer = 0

task.spawn(function()
    while true do
        task.wait(0.12)
        if Config.FarmSeaEvent then
            local char = Player.Character
            if char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Humanoid") and char.Humanoid.Health > 0 then
                local monster = (function()
                    local folders = { workspace:FindFirstChild("SeaMonster"), workspace:FindFirstChild("Monster") and workspace.Monster:FindFirstChild("Boss") }
                    for _, folder in pairs(folders) do
                        if folder then
                            for _, v in pairs(folder:GetChildren()) do
                                if (v.Name:find("Dragon") or v.Name:find("Crab") or v.Name:find("Hydra") or v.Name:find("Ship") or v.Name:find("FuryTentacle")) and v:FindFirstChild("HumanoidRootPart") then return v end
                            end
                        end
                    end
                    return nil
                end)()

                if monster then
                    if LastTargetName ~= monster.Name then
                        LastTargetName = monster.Name
                        SendWebhook("‚öîÔ∏è TARGET DETECTED", "üî• T·∫•n c√¥ng: **" .. monster.Name .. "**", 16711680)
                        ToggleMinimize(true)
                    end
                    
                    -- C·∫≠p nh·∫≠t Text Icon & To
                    if IsMinimized then Title.Text = "üéØ <font color='#FFA500'>" .. monster.Name .. "</font>" end
                    StatusLabel.Text = "‚öîÔ∏è <font color='rgb(255, 50, 50)'>" .. monster.Name:upper() .. "</font>"
                    
                    NoMonsterTimer = 0
                    char.HumanoidRootPart.CFrame = monster.HumanoidRootPart.CFrame * CFrame.new(0, 6, 8)
                    
                    local weapon = (function()
                        local t = (WeaponTypes[CurrentWeaponIdx] == "Sword") and "Sword" or "Combat"
                        local bp = Player:FindFirstChild("Backpack")
                        if bp then for _, tool in pairs(bp:GetChildren()) do if tool.ToolTip == t then return tool end end end
                        for _, tool in pairs(char:GetChildren()) do if tool:IsA("Tool") and tool.ToolTip == t then return tool end end
                        return nil
                    end)()

                    if weapon then
                        char.Humanoid:EquipTool(weapon)
                        task.spawn(function() RS.Chest.Remotes.Functions.SkillAction:InvokeServer((WeaponTypes[CurrentWeaponIdx] == "Sword") and "SW_Kioru V2_M1" or "FS_Trickster_M1", {MouseHit = monster.HumanoidRootPart.CFrame}) end)
                    end
                    CurrentWeaponIdx = (CurrentWeaponIdx == 1) and 2 or 1
                    
                    if Config.AutoSkill then
                        task.spawn(function()
                            for _, key in ipairs({"Z", "X", "C", "V", "B"}) do
                                if char and char:FindFirstChild("Humanoid") and char.Humanoid.Health > 0 then
                                    VIM:SendKeyEvent(true, key, false, game); task.wait(0.001); VIM:SendKeyEvent(false, key, false, game)
                                end
                            end
                        end)
                    end
                else
                    if LastTargetName ~= "" then
                        SendWebhook("üíÄ TARGET ELIMINATED", "Xong boss: " .. LastTargetName, 3447003)
                        LastTargetName = ""
                        ToggleMinimize(false)
                    end
                    
                    -- C·∫≠p nh·∫≠t Searching Icon
                    StatusLabel.Text = "üì° " .. math.floor(10 - NoMonsterTimer) .. "s"
                    StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
                    
                    if Config.AutoServerHop then
                        NoMonsterTimer = NoMonsterTimer + 0.12
                        if NoMonsterTimer >= 10 then 
                            local function ServerHop()
                                local url = "https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Desc&limit=100"
                                local success, result = pcall(function() return game:HttpGet(url) end)
                                if success then
                                    local servers = HttpService:JSONDecode(result).data
                                    for _, server in pairs(servers) do
                                        if server.playing < server.maxPlayers and server.id ~= game.JobId then
                                            TeleportService:TeleportToPlaceInstance(game.PlaceId, server.id, Player)
                                            return
                                        end
                                    end
                                end
                            end
                            ServerHop(); NoMonsterTimer = 0 
                        end
                    end
                end
            end
        end
    end
end)

Player.CharacterAdded:Connect(function()
    task.wait(1.5); pcall(function() RS.Chest.Remotes.Functions.EtcFunction:InvokeServer("EnterTheGame", {}) end)
    if Config.LowGraphics then ApplyLowGraphics() end
end)

task.spawn(function()
    pcall(function() RS.Chest.Remotes.Functions.EtcFunction:InvokeServer("EnterTheGame", {}) end)
    while task.wait(3) do
        if Config.FarmSeaEvent and Player.Character and Player.Character:FindFirstChild("Humanoid") and Player.Character.Humanoid.Health > 0 then
            pcall(function() if Config.AutoArm then RS.Chest.Remotes.Events.Armament:FireServer() end; if Config.AutoKen then RS.Chest.Remotes.Functions.KenEvent:InvokeServer() end end)
        end
    end
end)

task.spawn(function()
    local VirtualUser = game:GetService("VirtualUser")
    Player.Idled:Connect(function() VirtualUser:CaptureController(); VirtualUser:ClickButton2(Vector2.new()) end)
end)

if Config.BlackScreen then UpdateBlackScreen() end
