local SettingContainer = Instance.new("ScrollingFrame", ContentGroup)
SettingContainer.Size = UDim2.new(1, 0, 0.9, 0); SettingContainer.Position = UDim2.new(0, 0, 0.1, 0); SettingContainer.BackgroundTransparency = 1; SettingContainer.Visible = false; SettingContainer.ScrollBarThickness = 0

SeaTabBtn.MouseButton1Click:Connect(function() SeaContainer.Visible = true; SettingContainer.Visible = false; SeaTabBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 40); SettingTabBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 25) end)
SettingTabBtn.MouseButton1Click:Connect(function() SeaContainer.Visible = false; SettingContainer.Visible = true; SeaTabBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 25); SettingTabBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 40) end)

local function CreateToggle(text, pos, config_key, parent)
    local btn = Instance.new("TextButton", parent)
    btn.Size = UDim2.fromScale(0.9, 0.08); btn.Position = UDim2.fromScale(0.05, pos)
    local isOn = Config[config_key]
    btn.BackgroundColor3 = isOn and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 45)
    btn.Text = text .. ": " .. (isOn and "ON" or "OFF")
    btn.TextColor3 = isOn and Color3.new(1,1,1) or Color3.fromRGB(180,180,180)
    btn.Font = Enum.Font.GothamMedium; btn.TextSize = 13; Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)
    local Stroke = Instance.new("UIStroke", btn); Stroke.Color = isOn and Color3.fromRGB(0, 255, 150) or Color3.fromRGB(60,60,60)
    btn.MouseButton1Click:Connect(function()
        Config[config_key] = not Config[config_key]; local newState = Config[config_key]; SaveConfig()
        btn.BackgroundColor3 = newState and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 45)
        btn.TextColor3 = newState and Color3.new(1,1,1) or Color3.fromRGB(180,180,180)
        Stroke.Color = newState and Color3.fromRGB(0, 255, 150) or Color3.fromRGB(60,60,60)
        btn.Text = text .. ": " .. (newState and "ON" or "OFF")
    end)
end

